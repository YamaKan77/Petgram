<div class="form-wrapper">
	<%= simple_form_for @pic do |f| %>
		<div class="form-group">
	 		<%= f.input :image %>
		</div>
		<div class="form-group text-center">
		  <%= f.input :caption, input_html: {style: 'width:650px;'} %>
		</div>
		 <div class="form-group text-center">
		  <%= f.button :submit %>
		</div>
	<% end %>
</div>